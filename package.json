        "isomorphic-fetch": "^2.2.1",
        "material-ui": "^0.18.1",
        "node-sass": "^3.13.0",
        "object.values": "^1.0.4",
        "prop-types": "^15.5.10",
        "ramda": "^0.24.1",
        "react": "^15.4.2",
        "react-css-modules": "^4.1.0",
        "react-dom": "^15.4.1",
        "react-router-dom": "^4.0.0",
        "react-tap-event-plugin": "^2.0.1",
        "webpack-glob-entries": "^1.0.1"
    },
