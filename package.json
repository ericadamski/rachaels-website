{
    "name": "Rachaels-Website",
    "version": "1.0.0",
    "description": "",
    "main": "bundle.js",
    "scripts": {
        "prettier:code": "prettier --write --trailing-comma es5 --single-quote --tab-width 4 \"{app,lib}/**/*.js\"",
        "precommit": "lint-staged",
        "start": "webpack-dev-server --progress --inline --hot",
        "test": "jest --colors"
    },
    "author": "Eric Adamski",
    "engines": {
        "node": ">=6"
    },
    "license": "MIT",
    "jest": {
        "notify": false,
        "rootDir": "./",
        "testRegex": ".(spec).js[x]?$",
        "moduleFileExtensions": [
            "js",
            "jsx",
            "json"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/"
        ]
    },
    "pre-push": [
        "test"
    ],
