        "babel-preset-es2015": "^6.14.0",
        "babel-preset-react": "^6.11.1",
        "babel-preset-stage-2": "^6.22.0",
        "babel-preset-stage-3": "^6.17.0",
        "chai": "^3.5.0",
        "css-loader": "^0.26.1",
        "dotenv": "^4.0.0",
        "dotenv-webpack": "^1.4.3",
        "husky": "^0.13.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^19.0.2",
        "jest-cli": "^19.0.2",
        "json-loader": "^0.5.4",
        "lint-staged": "^3.4.2",
        "moment": "^2.18.1",
        "pre-push": "^0.1.1",
        "prettier": "^1.3.1",
        "raw-loader": "^0.5.1",
        "react-addons-test-utils": "^15.4.2",
        "sass-loader": "^4.0.2",
        "sinon": "^1.17.6",
        "style-loader": "^0.13.1",
        "webpack": "^2.2.1",
